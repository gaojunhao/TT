<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
<class name="com.test.mvc.model.MemberInfo" table="memberinfo" catalog="party">
<id name="MemberId" type="java.lang.Long">
<column name="member_id"/>
<generator class="identity"></generator>
</id>
<property name="MemberFact" type="java.lang.String">
<column name="member_fact" length="200" not-null="true" unique="true"/>
</property>
</class>
</hibernate-mapping>